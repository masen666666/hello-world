   "source": [
    "\n",
    "x,y,a=sy.symbols('x,y,a')\n",
    "sigma=5\n",
    "mean = 31.5\n",
    "gaussian=1/(sigma*sy.sqrt(2*sy.pi))*sy.E**(-(x-mean)**2/(2*sigma**2))\n",
    "Integ=sy.integrate(gaussian,(x,mean-a,mean+a))\n",
    "z1=sy.Eq(Integ,0.95)\n",
    "sy.pprint(sy.solve(z1))"
   ]
  }
